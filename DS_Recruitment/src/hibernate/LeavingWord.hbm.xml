<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.gdut.ds.beans.word.LeavingWord" table="LeavingWord" >
		<id name="id" column="id" type="string">
			<generator class="uuid">
			</generator>
		</id>
		<property name="name" column="name" type="string"></property>
		<property name="QQ" column="QQ" type="string"></property>
		<property name="email" column="email" type="string"></property>
		<property name="content" column="content" type="string"></property>
		<property name="answerOrNot" column="answerOrNot" type="boolean"></property>
	</class>

</hibernate-mapping>
 